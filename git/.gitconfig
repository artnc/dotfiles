[user]
	name = Art Chaidarun
	email = artchaidarun@gmail.com
[color]
	ui = true
[core]
	autocrlf = input
	editor = nano
	excludesfile = ~/.gitignore
	pager = less -+FX
[push]
	autoSetupRemote = true
	default = simple
[credential]
	helper = cache --timeout=864000
[init]
	templateDir = ~/.git-template
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
